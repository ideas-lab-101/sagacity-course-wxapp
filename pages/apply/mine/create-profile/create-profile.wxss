page{
    background-color: white;
}

.pro-box{
    padding: 50rpx 80rpx 0;
    background-color:#eae1df;
    position: relative;
}
.pro-box::after{
    content: "";
    position: absolute;
    z-index: -1;
    left: 0;
    bottom: -100rpx;
    width: 100%;
    height: 380rpx;
    background-color:#eae1df;
    border-radius: 600px / 100px;
}
.pro-info{
    text-align: center;
    display: flex;
    flex-direction: column;
    align-items: center;
}
.pro-info ._image{
    width: 200rpx;
    height: 200rpx;
    border-radius: 500rpx;
    display: inline-block;
    overflow: hidden;
}
.pro-info ._name{
    font-size: 40rpx;
    font-weight: 500;
    margin-top: 40rpx;
}
.pro-n{ margin-top: 40rpx;}
.pro-btn{
    background-color: #009b4c;
    background-image: -webkit-linear-gradient(0deg, #009b4c, #acce22);
    border: 0;
    line-height: 2.8;
    color: white;
    font-size: 32rpx;
    box-shadow: 0 6rpx 30rpx rgba(0, 155, 76, 0.5);
}
.pro-t{
    text-align: center; margin-top: 40rpx; font-size: 30rpx; font-weight: 500;
}
.pro-in{ position: relative; margin-top: 20rpx; padding: 0 40rpx; font-size: 24rpx; color: #999999; text-align: center;}
.pro-in>text{display: inline-block; vertical-align: middle; margin-top: -5rpx; margin-right: 10rpx;  width: 10rpx; height: 10rpx; border-radius: 100rpx; background-color:#009b4c; }
.pro-foot ._t{
    text-align: center; font-size: 30rpx; margin-top: 140rpx;
}
.pro-foot image{
    margin-top: 40rpx;
}


.enroll-fix{
    padding: 40rpx 20rpx;
}
.enroll-btn{
    text-align: center; padding: 25rpx; border-radius: 6rpx; border: 0 !important; color: white !important;
    line-height: 1.4;
    background-color:#009b4c;
    background-image:-webkit-linear-gradient(0deg, #009b4c, #acce22);
}

.ei-val-box{ display: flex; justify-content: space-between; align-items: flex-end; padding-top: 30rpx; }

.ei-val-box wux-input{ flex: 1; padding: 8rpx 0; }

.ei-val-box wux-input.def{ background-color: #f1f1f1; margin-right: 10rpx; padding: 8rpx 10rpx;}

.ei-val{ background-color: #eee; padding: 14rpx 25rpx; display: inline-block; font-size: 30rpx; }

.ei-group .wux-cell__text{ font-size: 22rpx; color: #999999;}