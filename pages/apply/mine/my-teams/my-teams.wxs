
var resolveTime = function (time) {
    var exp = getRegExp('-', 'g')
    var temp = time.replace(exp, '/')
    var d = getDate(temp)
    return d.getFullYear() + '年' + (d.getMonth() + 1) + '月' + d.getDate() + '日'
}

var getMembers = function (index, one, two, three) {
  if(index%3 == 0) {
    return one.members
  }else if(index%3 == 1) {
    return two.members
  }else if(index%3 == 2) {
    return three.members
  }else {
    return null
  }
}


